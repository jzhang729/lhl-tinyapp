<% if (!username) { %>
  <div id="login">
    <form action="/login" method="POST">
      <label for="username">Username</label>
      <input id="username" type="text" name="username" /><br />
      <input type="submit" />
    </form>
  </div>
<% } else { %>
  <p><%= username %></p>
  <form action="/logout" method="POST">
    <input type="submit" value="Log Out" />
  </form>
<% } %>
